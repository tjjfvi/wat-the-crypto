{
  "incremental": true,
  "indentWidth": 2,
  "lineWidth": 100,
  "typescript": {
    "quoteProps": "asNeeded",
    "arrowFunction.useParentheses": "force",
    "semiColons": "asi"
  },
  "includes": ["**.{dockerfile,json,md,toml,ts,wat}"],
  "excludes": ["target"],
  "plugins": [
    "https://plugins.dprint.dev/typescript-0.65.1.wasm",
    "https://plugins.dprint.dev/json-0.14.1.wasm",
    "https://plugins.dprint.dev/markdown-0.12.2.wasm",
    "https://plugins.dprint.dev/toml-0.5.4.wasm",
    "https://plugins.dprint.dev/dockerfile-0.2.2.wasm",
    "https://raw.githubusercontent.com/tjjfvi/form.wat/v0.1.0/form.wasm"
  ]
}
